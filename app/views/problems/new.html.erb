<h1>Problem</h1>
<%= form_for :problem, url: problems_path do |f| %>

<p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
 <%= f.label :Picture %><br>
    <%= f.file_field :Picture %>
    </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
 
<%= link_to 'Back', problems_path %>